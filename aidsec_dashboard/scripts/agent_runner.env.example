# Shared API endpoint (local machine over Tailscale)
AIDSEC_API_BASE_URL=http://100.100.97.120:8000/api

# Global backend API key (must match API_KEY on backend)
AIDSEC_GLOBAL_API_KEY=replace_with_global_api_key

# Unique agent identity and key
AIDSEC_AGENT_ID=agent1
AIDSEC_AGENT_API_KEY=replace_with_agent_specific_key

# Timing settings
AIDSEC_LEASE_SECONDS=300
AIDSEC_HEARTBEAT_INTERVAL_SECONDS=60
AIDSEC_POLL_INTERVAL_SECONDS=10
AIDSEC_REQUEST_TIMEOUT_SECONDS=30
AIDSEC_AGENT_LOG_LEVEL=INFO
